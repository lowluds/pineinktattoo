{
  "name": "pineink-tattoo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "export": "next build && next export",
    "performance": "npm run build && npx lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "bundle-analyzer": "npm run build && npx @next/bundle-analyzer",
    "performance-budget": "npm run build && npx webpack-bundle-analyzer .next/static/chunks/*.js",
    "lighthouse": "npx lighthouse http://localhost:3000 --only-categories=performance --output=json --output-path=./lighthouse-performance.json",
    "core-web-vitals": "npm run lighthouse && node scripts/check-core-web-vitals.js"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.534.0",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/language-server": "^0.14.25",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "lighthouse": "^11.6.0",
    "@next/bundle-analyzer": "^15.4.5",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
